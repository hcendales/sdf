	<parameter name="P_CONCESIONARIO" isForPrompting="true" class="java.lang.String"/>
	<parameter name="P_BANCO" isForPrompting="true" class="java.lang.String"/>
	<parameter name="P_TIPO" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select
       recaudo_local_validacion.indorigen "Origen",
       recaudo_local_validacion.indTipo "Tipo",
       recaudo_local_validacion.idbanco,
       banco.nombre "Banco",
       recaudo_local_validacion.idconcesionario,      
       concesionario.nombre "Operador",
       recaudo_local_validacion.idMunicipio "DIVIPO",
       municipio.nombre "Municipio",
       departamento.nombre "Departamento",
       recaudo_local_validacion.cta_recaudo "Cuenta Recaudo",
       recaudo_local_validacion.fec_corte "Fecha Corte",
       recaudo_local_validacion.fec_tran "Fecha Transaccion",
       recaudo_local_validacion.oficina_origen "Oficina",
       recaudo_local_validacion.valor_total "Vr_Recaudo",
       recaudo_local_validacion.fec_dispersion "Fecha Dispersion",
       recaudo_local_validacion.p_consignado "pConsignado",
       recaudo_local_validacion.vigencia "Vigencia",
       recaudo_local_validacion.fec_vig_ini "fec_vig_ini",
       recaudo_local_validacion.fec_vig_fin "fec_vig_fin"

  from recaudo_local_validacion,
       banco,
       municipio,
       departamento,
       concesionario
 where banco.idbanco = recaudo_local_validacion.idbanco
       and municipio.idmunicipio = recaudo_local_validacion.idmunicipio
       and departamento.iddepartamento = recaudo_local_validacion.iddepartamento
       and concesionario.idconcesionario = recaudo_local_validacion.idconcesionario
       and recaudo_local_validacion.indtipo = decode ($P{P_TIPO},'TODOS',recaudo_local_validacion.indtipo,$P{P_TIPO})
       and TO_CHAR(recaudo_local_validacion.fec_corte,'YYYY/MM/DD') >= $P{P_FEC_INI}
       and TO_CHAR(recaudo_local_validacion.fec_corte,'YYYY/MM/DD') <= $P{P_FEC_FIN}
       and ( recaudo_local_validacion.idconcesionario = decode($P{P_CONCESIONARIO},'|99', recaudo_local_validacion.idconcesionario, '') or
             instr($P{P_CONCESIONARIO}, recaudo_local_validacion.idconcesionario) > 0
	   )

       and ( recaudo_local_validacion.idbanco = decode($P{P_BANCO},'|99', recaudo_local_validacion.idbanco, '' ) or
             instr($P{P_BANCO}, recaudo_local_validacion.idbanco) > 0
	   )

 order by  recaudo_local_validacion.idbanco, recaudo_local_validacion.idconcesionario, recaudo_local_validacion.idmunicipio, recaudo_local_validacion.fec_corte]]></queryString>

